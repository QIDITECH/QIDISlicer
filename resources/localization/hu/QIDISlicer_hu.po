#: src/slic3r/GUI/Tab.cpp:5820
msgid "Supports"
msgstr "Támasztékok"

#: src/libslic3r/PrintConfig.cpp:3762
msgid "Diameter of the pointing side of the head"
msgstr "A fej mutató oldalának átmérője"

#: src/libslic3r/PrintConfig.cpp:3769
