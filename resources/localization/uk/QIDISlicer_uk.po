msgstr "Вирівняно"

#: src/libslic3r/PrintConfig.cpp:2716 src/slic3r/GUI/MainFrame.cpp:1422
msgid "Rear"
msgstr "Ззаду"

#: src/libslic3r/PrintConfig.cpp:2722
msgid "Staggered inner seams"
msgstr ""

