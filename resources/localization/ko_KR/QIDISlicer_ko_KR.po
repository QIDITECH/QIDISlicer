msgstr "선호하는 재봉선(seam)의 방향"

#: src/libslic3r/PrintConfig.cpp:2797
msgid "Seam preferred direction"
msgstr "재봉선(Seam) 선호 방향"

#: src/libslic3r/PrintConfig.cpp:2804
msgid "Jitter"
msgstr "지터(Jitter)"

#: src/libslic3r/PrintConfig.cpp:2806
