msgid "Tilt"
msgstr "Inclinación"

#: src/libslic3r/PrintConfig.cpp:3739
msgid "Tilt for high viscosity resin"
msgstr "Inclinación para resina de alta viscosidad"

#: src/slic3r/GUI/Tab.cpp:2779
msgid "Tilt time"
msgstr "Tiempo de inclinación"

