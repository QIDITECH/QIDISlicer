#: src/libslic3r/PrintConfig.cpp:6005
msgid "Number of printing extruders"
msgstr "Nombre d'extrusores d'impressió"

#: src/libslic3r/PrintConfig.cpp:6006
msgid "Number of extruders used during the print."
