#: src/libslic3r/PrintConfig.cpp:676
msgid "Brim width"
msgstr "Шырыня аблямоўкі"

#: src/libslic3r/PrintConfig.cpp:677 src/libslic3r/PrintConfig.cpp:688
#: src/libslic3r/PrintConfig.cpp:701 src/slic3r/GUI/GUI_Factories.cpp:141
